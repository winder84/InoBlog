<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table comment="ino_blog_article Table" engine="innodb" name="ino_blog_article" resource="default">
		<column comment="Entity Id" identity="true" name="article_id" nullable="false" padding="6" unsigned="true" xsi:type="smallint"/>
		<constraint referenceId="PRIMARY" xsi:type="primary">
			<column name="article_id"/>
		</constraint>
		<column length="255" name="title" nullable="false" xsi:type="text"/>
		<column name="small_description" nullable="true" xsi:type="text"/>
		<column name="description" nullable="true" xsi:type="text"/>
	</table>
</schema>
